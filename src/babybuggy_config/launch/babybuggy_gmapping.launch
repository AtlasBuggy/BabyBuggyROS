<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <node pkg="gmapping" type="slam_gmapping" name="slam_gmapping" output="screen">
        <rosparam>
            odom_frame: odom
            base_frame: base_link
            map_frame: map

            map_update_interval: 10  # Publish new map (seconds)
            throttle_scans: 1  # don't skip any scans

            maxRange: 32.76 # Max sensor value
            maxUrange: 32.0 # Usable region of the sensor
            particles: 80

            # minimumScore: 5

            # Update frequencies
            # linearUpdate: 0.01
            # angularUpdate: 0.01
            temporalUpdate: 0.01
            # resampleThreshold: 0.5

            # srr: 0.01
            # srt: 0.01
            # str: 0.01
            # stt: 0.01

            # Initial Map Size
            xmin: -25.0
            ymin: -25.0
            xmax: 25.0
            ymax: 25.0
            delta: 0.05

            # All default
            # sigma: 0.05
            # kernelSize: 1
            # lstep: 0.05
            # astep: 0.05
            # iterations: 5
            # lsigma: 0.075
            # ogain: 3.0
            # lskip: 0
            # llsamplerange: 0.01
            # llsamplestep: 0.01
            # lasamplerange: 0.005
            # lasamplestep: 0.005

        </rosparam>
    </node>
</launch>
